{
  "name": "pw512/php-berkeley-db",
  "description": "A simple Berkeley DB library for PHP.",
  "type": "library",
  "keywords": [
    "berkeley",
    "database",
    "db",
    "db4"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Komarov"
    }
  ],
  "require": {
    "php": "^7.0",
    "ext-dba": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0"
  },
  "autoload": {
    "psr-4": {
      "AndKom\\BerkeleyDb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndKom\\BerkeleyDb\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AndKom\\BerkeleyDb\\ServiceProvider"
      ],
      "aliases": {
        "BDB": "AndKom\\BerkeleyDb\\Facade\\BDB"
      }
    }
  }
}
